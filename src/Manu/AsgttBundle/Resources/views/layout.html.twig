</head>
<body class="accueil">
<div class="user" hidden>
	<span class="type-user">
		{% if is_admin() %}
			ADMIN
		{% elseif is_joueur() %}
			JOUEUR
		{% else %}
			ANON
		{% endif %}
	</span>
</div>
 <div class="container-fluid page">
 	
 	<!-- cette partie sera visible sur grand ecran et remplac�e par une navbar bootstrap -->
 	<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 header row">
 		
 		<div class="logo col-lg-4 col-md-4 col-sm-4">
 			<a href="#"><img alt="logo" src="/bundles/manuasgtt/images/ASGTT-Logo_page_web.png"/></a>
 		</div>
 		<div class="main_menu col-lg-8 col-md-8  col-sm-8 row">
 			<ul>
 				<li class="col-lg-2 col-md-2 col-sm-2"><a href="#" ><img alt="club" src="/bundles/manuasgtt/images/ASGTT - Rond Le Club.png" /></a>
 					<ul class="submenu">
 						<li><a href="#maire">Le mot du maire</a></li>
 						<li><a href="#entrainements">Les entrainements</a></li>
 						<li><a href="#bureau">Le bureau</a></li>
 					</ul>
 				</li>
 				<li class="col-lg-2 col-md-2 col-sm-2"><a href="#"><img alt="joueurs" src="/bundles/manuasgtt/images/ASGTT - Rond Joueurs.png" /></a>
 					<ul class="submenu">
 						<li><a href="#tradi">Tradi</a></li>
 						<li><a href="#promo">Promo</a>
 					</ul>
 				</li>
 				<li class="col-lg-2 col-md-2 col-sm-2 "><a href="#photos"><img alt="photos" src="/bundles/manuasgtt/images/ASGTT - Rond Photos.png" /></a>
 				<li class="col-lg-2 col-md-2 col-sm-2"><a href="#contact"><img alt="contact" src="/bundles/manuasgtt/images/ASGTT - Rond Contact.png" /></a>
 				<li class="col-lg-2 col-md-2 col-sm-2"><a href="#compteur"><img alt="compteur" src="/bundles/manuasgtt/images/ASGTT - Rond Compteur points.png" /></a>
 				<li class="col-lg-2 col-md-2 col-sm-2"><a href="#membres"><img alt="compteur" src="/bundles/manuasgtt/images/ASGTT - Rond Espace membres.png" /></a>
 			</ul>
 		</div>
 	</div>
 	
	<!-- A AFFICHER SEULEMENT SUR MOBILE -->
	<nav class="navbar navbar-default">
	  <div class="container-fluid">
	    <!-- Brand and toggle get grouped for better mobile display -->
	    <div class="navbar-header">
	      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
	        <span class="sr-only">Toggle navigation</span>
	        <span class="icon-bar"></span>
	        <span class="icon-bar"></span>
	        <span class="icon-bar"></span>
	      </button>
	      <a class="navbar-brand" href="#">
	      	<img alt="Brand" src="/bundles/manuasgtt/images/littleLogo.png">
	      </a>
	    </div>
	
	    <!-- Collect the nav links, forms, and other content for toggling -->
	    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
	      <ul class="nav navbar-nav">
	        <li class="dropdown">
	          <a class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Le Club <span class="caret"></span></a>
	          <ul class="dropdown-menu">
	            <li><a href="#maire">Le mot du Maire</a></li>
	            <li><a href="#entrainements">Les Entrainements</a></li>
	            <li><a href="#bureau">Les Bureau</a></li>
	          </ul>
	        </li>
	         <li><a href="#photos">Photos</a></li>
	         <li><a href="#contact">Contact</a></li>
	         <li><a href="#compteurs">Compteur de points</a></li>
	         <li><a href="#membres">Espace Membre</a></li>
	        
	        <li class="dropdown">
	          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Les joueurs <span class="caret"></span></a>
	          <ul class="dropdown-menu">
	            <li><a href="#tradi">Tradi</a></li>
	            <li><a href="#promo">Promo</a></li>
	          </ul>
	        </li>
	      </ul>
	    </div><!-- /.navbar-collapse -->
	  </div><!-- /.container-fluid -->
	</nav>
 	
 	
 	<div class="content col-md-12 col-sm-12 col-xs-12 row">
	 	<div class="page_accueil"> 
	 		<div class="bienvenue ">
				&nbsp; 		
	 		</div>
	 		<!-- CARIOUSSEL -->
	        <div class=" slider zoom row">
	        
	            <img src="/bundles/manuasgtt/images/ASGTT-Logo_page_web.png" width="100%" style="width: 100%"/>
	             <img src="/bundles/manuasgtt/images/test.jpg" width="100%" style="width: 100%"/>
	        </div>
			
			
			        
	        <div class="liste_articles row" >
	 		{% if articles is defined %}
	 			{% for article in articles %}
	 				
                    <div class="col-lg-10 col-md-10 col-sm-12 col-xs-12 col-lg-offset-1 col-md-offset-1 articles " article_id="{{article.id}}">
                         {% if is_admin() %} 
                             <button type="button" class="btn btn-default button_remove" aria-label="Left Align">
                                <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                             </button>
                              <span class="titre_article col-lg-12 col-md-12 col-sm-12 col-xs-12">{{ article.titre }}</span> <br/>
                              <span class="content_article col-lg-12 col-md-12 col-sm-12 col-xs-12">&nbsp;&nbsp;&nbsp;&nbsp;{{ article.contenu|raw }}</span>
	 							<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                              	<button class="col-lg-3 col-md-3 col-sm-6 col-xs-6 col-lg-offset-1 col-md-offset-1 button_update">Modifier</button>
                              </div>
                          {% else %}
                          	  <span class="titre_article col-lg-12 col-md-12 col-sm-12 col-xs-12">{{ article.titre }}</span> <br/>
                                  <span class="content_article col-lg-12 col-md-12 col-sm-12 col-xs-12">&nbsp;&nbsp;&nbsp;&nbsp;{{ article.contenu|raw }}</span>
                         {% endif %}
                                                
	 					</div>
	 			{% endfor %}
	 		{% endif %}
	 		</div>
	 	</div>
	 	
	 <!-- PARTIE DES PAGES GERER EN CSS -->
	 
	 <div class="page_maire row">
	 	   {% if is_admin() %}
	 	   		<textarea name="editor_maire" class="editor col-lg-12 col-md-12 col-sm-12 col-xs-12">
	 	   		
	 	   		</textarea>
	 	   		<br>
	 	   		<button type="button" class="col-lg-2 col-md-2 col-sm-2 col-xs-2 btn" onclick="document.location.href='#maire/save'">Enregistrer</button>
	 	   {% else %}
	 	   		<div class="content col-lg-12 col-md-12 col-sm-12 col-xs-12">
	 	   			
	 	   		</div>
	 	   
	 	   {% endif %}
     </div>
 	 
 	 <div class="page_entrainements row">
			{% if is_admin() %}
	 	   		<textarea name="editor_entrainements" class="editor col-lg-12 col-md-12 col-sm-12 col-xs-12">
	 	   		
	 	   		</textarea>
	 	   		<br>
	 	   		<button type="button" class="col-lg-2 col-md-2 col-sm-2 col-xs-2 btn" onclick="document.location.href='#entrainements/save'">Enregistrer</button>
	 	   {% else %}
	 	   		<div class="content col-lg-12 col-md-12 col-sm-12 col-xs-12">
	 	   			
	 	   		</div>
	 	   
	 	   {% endif %}
     </div>
     
     <div class="page_bureau row">
			{% if is_admin() %}
	 	   		<textarea name="editor_bureau" class="editor col-lg-12 col-md-12 col-sm-12 col-xs-12">
	 	   		
	 	   		</textarea>
	 	   		<br>
	 	   		<button type="button" class="col-lg-2 col-md-2 col-sm-2 col-xs-2 btn" onclick="document.location.href='#bureau/save'">Enregistrer</button>
	 	   {% else %}
	 	   		<div class="content col-lg-12 col-md-12 col-sm-12 col-xs-12">
	 	   			
	 	   		</div>
	 	   {% endif %}
     </div>
     
     <div class="page_tradi row">
		page_tradi
     </div>
     
     <div class="page_promo row">
		page_promo
     </div>
     
     <div class="page_photos row">
		page_photos
     </div>
     
     <div class="page_contact row">
		page_contact
     </div>
     
     <div class="page_compteur row">
		page_compteur
     </div>
     
     <div class="page_membres row">
		page_membres
     </div>
 	</div>
 	
 	
 	<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 footer">
 		&nbsp;
 	</div>
 	<script>
		
		CKEDITOR.replace( 'editor_entrainements');
		CKEDITOR.replace( 'editor_maire');
		CKEDITOR.replace( 'editor_bureau');
 	</script>
 </div>

  </body>
</html>